{
  "manifest_version": 2,
  "version": "0.1",

  "name": "RT video Helper",
  "description": "This extension helps you keep track of the videos you've seen on RoosterTeeth.com.",
  "version": "1.0",

    "content_scripts": [
        {
            "matches": ["http://*.roosterteeth.com/*", "http://*.achievementhunter.com/*"],
            "js": ["jquery.js", "rthelper.js"]
        }
    ],
  "web_accessible_resources": [
        "images/seen.png"
  ]
}
